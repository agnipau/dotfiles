! URxvt
*foreground: base07
*background: base00
*cursorColor: base07

*color0: base00
*color1: base01
*color2: base02
*color3: base03
*color4: base04
*color5: base05
*color6: base06
*color7: base07

*color8: base08
*color9: base09
*color10: base0A
*color11: base0B
*color12: base0C
*color13: base0D
*color14: base0E
*color15: base0F

URxvt.cursorBlink: 1
URxvt.font: xft:Input Mono Narrow:size=14:style=Regular
URxvt.italicFont: xft:Input Mono Narrow:size=14:style=Italic
URxvt.boldFont: xft:Input Mono Narrow:size=14:style=Bold

URxvt.lineSpace: 0
URxvt.letterSpace: -1
URxvt.scrollBar: false
URxvt.internalBorder: 44
URxvt.cursorUnderline: false
URxvt.geometry: 70x35
URxvt.thickness: 5
URxvt.titleBar: false
URxvt.intensityStyles: false

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: chromium
URxvt.matcher.button: 1

! Rofi
rofi.color-window: base00 , base07 , base00 , base00 , base03
rofi.color-normal: base00 , base08 , base00 , base00 , base07
rofi.color-active: base08 , base0E , base00 , base0B , base00
rofi.color-urgent: base00 , base08 , base00 , base00 , base08

rofi.font: Input Mono Narrow 13
rofi.matching: fuzzy
rofi.levenshtein-sort: true
rofi.separator-style: none
rofi.hide-scrollbar: true
rofi.padding: 32
rofi.bw: 2

! sxiv
Sxiv.font: Input Mono Narrow 16
Sxiv.background: #282828
Sxiv.foreground: #ebdbb2

! General
Xft.autohint: 1
Xft.antialias: 1
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: true
Xft.rgba: rgb
*.dpi: 125
! *.dpi: 96

