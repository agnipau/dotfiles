{
  "diagnostic.messageTarget": "float",
  "diagnostic.messageDelay": 150,
  "suggest.timeout": 500,
  "languageserver": {
      "dart": {
          "command": "dart",
          "args": ["/opt/dart-sdk/bin/snapshots/analysis_server.dart.snapshot", "--lsp"],
          "filetypes": ["dart"]
      },
      "rust": {
          "command": "rust-analyzer",
          "filetypes": ["rust"]
      },
      "python": {
          "command": "pyls",
          "filetypes": ["python"]
      }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "rust",
    "dart",
    "typescript",
    "typescriptreact",
    "python"
  ]
}

