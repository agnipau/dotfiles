#!/usr/bin/env bash
#
# A script that sources the colors generated by pywal, creates 20%, 30%, 40%
# 50%, 60% lighter and darker variants of them and finally adds them to:
#
# ~/.cache/wal/colors.css
# ~/.cache/wal/colors.json
# ~/.cache/wal/colors-putty.reg
# ~/.cache/wal/colors.scss
# ~/.cache/wal/colors.sh
# ~/.cache/wal/colors-sway
# ~/.cache/wal/colors-wal.vim
# ~/.cache/wal/colors.Xresources
# ~/.cache/wal/colors.yml

. "${HOME}/.cache/wal/colors.sh"

# Create lighter and darker variants of all the colors generated by pywal.
gen_colors() {
  for i in {0..15}; do
    color="color${i}"
    c_rgb_12d="$(( 0x"${!color:1:2}" ))"
    c_rgb_34d="$(( 0x"${!color:3:2}" ))"
    c_rgb_56d="$(( 0x"${!color:5:2}" ))"

    c_rgb_12d_10l="$(( c_rgb_12d + "$((c_rgb_12d / 10))" ))"
    c_rgb_34d_10l="$(( c_rgb_34d + "$((c_rgb_34d / 10))" ))"
    c_rgb_56d_10l="$(( c_rgb_56d + "$((c_rgb_56d / 10))" ))"
    [[ "${c_rgb_12d_10l}" -ge 255 ]] && c_rgb_12d_10l=255
    [[ "${c_rgb_34d_10l}" -ge 255 ]] && c_rgb_34d_10l=255
    [[ "${c_rgb_56d_10l}" -ge 255 ]] && c_rgb_56d_10l=255
    printf -v c_hex_12d_10l '%x' "$c_rgb_12d_10l"
    printf -v c_hex_34d_10l '%x' "$c_rgb_34d_10l"
    printf -v c_hex_56d_10l '%x' "$c_rgb_56d_10l"
    [[ "${#c_hex_12d_10l}" -eq 1 ]] && c_hex_12d_10l="0${c_hex_12d_10l}"
    [[ "${#c_hex_34d_10l}" -eq 1 ]] && c_hex_34d_10l="0${c_hex_34d_10l}"
    [[ "${#c_hex_56d_10l}" -eq 1 ]] && c_hex_56d_10l="0${c_hex_56d_10l}"
    c_hex_10l="#${c_hex_12d_10l}${c_hex_34d_10l}${c_hex_56d_10l}"
    c_rgb_10l="${c_rgb_12d_10l},${c_rgb_34d_10l},${c_rgb_56d_10l}"
    declare -g color"${i}"_lighter_10="$c_hex_10l"
    declare -g color"${i}"_lighter_10_rgb="$c_rgb_10l"

    c_rgb_12d_20l="$(( c_rgb_12d + "$((c_rgb_12d / 5))" ))"
    c_rgb_34d_20l="$(( c_rgb_34d + "$((c_rgb_34d / 5))" ))"
    c_rgb_56d_20l="$(( c_rgb_56d + "$((c_rgb_56d / 5))" ))"
    [[ "${c_rgb_12d_20l}" -ge 255 ]] && c_rgb_12d_20l=255
    [[ "${c_rgb_34d_20l}" -ge 255 ]] && c_rgb_34d_20l=255
    [[ "${c_rgb_56d_20l}" -ge 255 ]] && c_rgb_56d_20l=255
    printf -v c_hex_12d_20l '%x' "$c_rgb_12d_20l"
    printf -v c_hex_34d_20l '%x' "$c_rgb_34d_20l"
    printf -v c_hex_56d_20l '%x' "$c_rgb_56d_20l"
    [[ "${#c_hex_12d_20l}" -eq 1 ]] && c_hex_12d_20l="0${c_hex_12d_20l}"
    [[ "${#c_hex_34d_20l}" -eq 1 ]] && c_hex_34d_20l="0${c_hex_34d_20l}"
    [[ "${#c_hex_56d_20l}" -eq 1 ]] && c_hex_56d_20l="0${c_hex_56d_20l}"
    c_hex_20l="#${c_hex_12d_20l}${c_hex_34d_20l}${c_hex_56d_20l}"
    c_rgb_20l="${c_rgb_12d_20l},${c_rgb_34d_20l},${c_rgb_56d_20l}"
    declare -g color"${i}"_lighter_20="$c_hex_20l"
    declare -g color"${i}"_lighter_20_rgb="$c_rgb_20l"

    c_rgb_12d_30l="$(( c_rgb_12d + "$((c_rgb_12d * 3 / 10))" ))"
    c_rgb_34d_30l="$(( c_rgb_34d + "$((c_rgb_34d * 3 / 10))" ))"
    c_rgb_56d_30l="$(( c_rgb_56d + "$((c_rgb_56d * 3 / 10))" ))"
    [[ "${c_rgb_12d_30l}" -ge 255 ]] && c_rgb_12d_30l=255
    [[ "${c_rgb_34d_30l}" -ge 255 ]] && c_rgb_34d_30l=255
    [[ "${c_rgb_56d_30l}" -ge 255 ]] && c_rgb_56d_30l=255
    printf -v c_hex_12d_30l '%x' "$c_rgb_12d_30l"
    printf -v c_hex_34d_30l '%x' "$c_rgb_34d_30l"
    printf -v c_hex_56d_30l '%x' "$c_rgb_56d_30l"
    [[ "${#c_hex_12d_30l}" -eq 1 ]] && c_hex_12d_30l="0${c_hex_12d_30l}"
    [[ "${#c_hex_34d_30l}" -eq 1 ]] && c_hex_34d_30l="0${c_hex_34d_30l}"
    [[ "${#c_hex_56d_30l}" -eq 1 ]] && c_hex_56d_30l="0${c_hex_56d_30l}"
    c_hex_30l="#${c_hex_12d_30l}${c_hex_34d_30l}${c_hex_56d_30l}"
    c_rgb_30l="${c_rgb_12d_30l},${c_rgb_34d_30l},${c_rgb_56d_30l}"
    declare -g color"${i}"_lighter_30="$c_hex_30l"
    declare -g color"${i}"_lighter_30_rgb="$c_rgb_30l"

    c_rgb_12d_40l="$(( c_rgb_12d + "$((c_rgb_12d * 2 / 5))" ))"
    c_rgb_34d_40l="$(( c_rgb_34d + "$((c_rgb_34d * 2 / 5))" ))"
    c_rgb_56d_40l="$(( c_rgb_56d + "$((c_rgb_56d * 2 / 5))" ))"
    [[ "${c_rgb_12d_40l}" -ge 255 ]] && c_rgb_12d_40l=255
    [[ "${c_rgb_34d_40l}" -ge 255 ]] && c_rgb_34d_40l=255
    [[ "${c_rgb_56d_40l}" -ge 255 ]] && c_rgb_56d_40l=255
    printf -v c_hex_12d_40l '%x' "$c_rgb_12d_40l"
    printf -v c_hex_34d_40l '%x' "$c_rgb_34d_40l"
    printf -v c_hex_56d_40l '%x' "$c_rgb_56d_40l"
    [[ "${#c_hex_12d_40l}" -eq 1 ]] && c_hex_12d_40l="0${c_hex_12d_40l}"
    [[ "${#c_hex_34d_40l}" -eq 1 ]] && c_hex_34d_40l="0${c_hex_34d_40l}"
    [[ "${#c_hex_56d_40l}" -eq 1 ]] && c_hex_56d_40l="0${c_hex_56d_40l}"
    c_hex_40l="#${c_hex_12d_40l}${c_hex_34d_40l}${c_hex_56d_40l}"
    c_rgb_40l="${c_rgb_12d_40l},${c_rgb_34d_40l},${c_rgb_56d_40l}"
    declare -g color"${i}"_lighter_40="$c_hex_40l"
    declare -g color"${i}"_lighter_40_rgb="$c_rgb_40l"

    c_rgb_12d_50l="$(( c_rgb_12d + "$((c_rgb_12d / 2))" ))"
    c_rgb_34d_50l="$(( c_rgb_34d + "$((c_rgb_34d / 2))" ))"
    c_rgb_56d_50l="$(( c_rgb_56d + "$((c_rgb_56d / 2))" ))"
    [[ "${c_rgb_12d_50l}" -ge 255 ]] && c_rgb_12d_50l=255
    [[ "${c_rgb_34d_50l}" -ge 255 ]] && c_rgb_34d_50l=255
    [[ "${c_rgb_56d_50l}" -ge 255 ]] && c_rgb_56d_50l=255
    printf -v c_hex_12d_50l '%x' "$c_rgb_12d_50l"
    printf -v c_hex_34d_50l '%x' "$c_rgb_34d_50l"
    printf -v c_hex_56d_50l '%x' "$c_rgb_56d_50l"
    [[ "${#c_hex_12d_50l}" -eq 1 ]] && c_hex_12d_50l="0${c_hex_12d_50l}"
    [[ "${#c_hex_34d_50l}" -eq 1 ]] && c_hex_34d_50l="0${c_hex_34d_50l}"
    [[ "${#c_hex_56d_50l}" -eq 1 ]] && c_hex_56d_50l="0${c_hex_56d_50l}"
    c_hex_50l="#${c_hex_12d_50l}${c_hex_34d_50l}${c_hex_56d_50l}"
    c_rgb_50l="${c_rgb_12d_50l},${c_rgb_34d_50l},${c_rgb_56d_50l}"
    declare -g color"${i}"_lighter_50="$c_hex_50l"
    declare -g color"${i}"_lighter_50_rgb="$c_rgb_50l"

    c_rgb_12d_60l="$(( c_rgb_12d + "$((c_rgb_12d * 3 / 5))" ))"
    c_rgb_34d_60l="$(( c_rgb_34d + "$((c_rgb_34d * 3 / 5))" ))"
    c_rgb_56d_60l="$(( c_rgb_56d + "$((c_rgb_56d * 3 / 5))" ))"
    [[ "${c_rgb_12d_60l}" -ge 255 ]] && c_rgb_12d_60l=255
    [[ "${c_rgb_34d_60l}" -ge 255 ]] && c_rgb_34d_60l=255
    [[ "${c_rgb_56d_60l}" -ge 255 ]] && c_rgb_56d_60l=255
    printf -v c_hex_12d_60l '%x' "$c_rgb_12d_60l"
    printf -v c_hex_34d_60l '%x' "$c_rgb_34d_60l"
    printf -v c_hex_56d_60l '%x' "$c_rgb_56d_60l"
    [[ "${#c_hex_12d_60l}" -eq 1 ]] && c_hex_12d_60l="0${c_hex_12d_60l}"
    [[ "${#c_hex_34d_60l}" -eq 1 ]] && c_hex_34d_60l="0${c_hex_34d_60l}"
    [[ "${#c_hex_56d_60l}" -eq 1 ]] && c_hex_56d_60l="0${c_hex_56d_60l}"
    c_hex_60l="#${c_hex_12d_60l}${c_hex_34d_60l}${c_hex_56d_60l}"
    c_rgb_60l="${c_rgb_12d_60l},${c_rgb_34d_60l},${c_rgb_56d_60l}"
    declare -g color"${i}"_lighter_60="$c_hex_60l"
    declare -g color"${i}"_lighter_60_rgb="$c_rgb_60l"

    c_rgb_12d_70l="$(( c_rgb_12d + "$((c_rgb_12d * 7 / 10))" ))"
    c_rgb_34d_70l="$(( c_rgb_34d + "$((c_rgb_34d * 7 / 10))" ))"
    c_rgb_56d_70l="$(( c_rgb_56d + "$((c_rgb_56d * 7 / 10))" ))"
    [[ "${c_rgb_12d_70l}" -ge 255 ]] && c_rgb_12d_70l=255
    [[ "${c_rgb_34d_70l}" -ge 255 ]] && c_rgb_34d_70l=255
    [[ "${c_rgb_56d_70l}" -ge 255 ]] && c_rgb_56d_70l=255
    printf -v c_hex_12d_70l '%x' "$c_rgb_12d_70l"
    printf -v c_hex_34d_70l '%x' "$c_rgb_34d_70l"
    printf -v c_hex_56d_70l '%x' "$c_rgb_56d_70l"
    [[ "${#c_hex_12d_70l}" -eq 1 ]] && c_hex_12d_70l="0${c_hex_12d_70l}"
    [[ "${#c_hex_34d_70l}" -eq 1 ]] && c_hex_34d_70l="0${c_hex_34d_70l}"
    [[ "${#c_hex_56d_70l}" -eq 1 ]] && c_hex_56d_70l="0${c_hex_56d_70l}"
    c_hex_70l="#${c_hex_12d_70l}${c_hex_34d_70l}${c_hex_56d_70l}"
    c_rgb_70l="${c_rgb_12d_70l},${c_rgb_34d_70l},${c_rgb_56d_70l}"
    declare -g color"${i}"_lighter_70="$c_hex_70l"
    declare -g color"${i}"_lighter_70_rgb="$c_rgb_70l"

    c_rgb_12d_80l="$(( c_rgb_12d + "$((c_rgb_12d * 4 / 5))" ))"
    c_rgb_34d_80l="$(( c_rgb_34d + "$((c_rgb_34d * 4 / 5))" ))"
    c_rgb_56d_80l="$(( c_rgb_56d + "$((c_rgb_56d * 4 / 5))" ))"
    [[ "${c_rgb_12d_80l}" -ge 255 ]] && c_rgb_12d_80l=255
    [[ "${c_rgb_34d_80l}" -ge 255 ]] && c_rgb_34d_80l=255
    [[ "${c_rgb_56d_80l}" -ge 255 ]] && c_rgb_56d_80l=255
    printf -v c_hex_12d_80l '%x' "$c_rgb_12d_80l"
    printf -v c_hex_34d_80l '%x' "$c_rgb_34d_80l"
    printf -v c_hex_56d_80l '%x' "$c_rgb_56d_80l"
    [[ "${#c_hex_12d_80l}" -eq 1 ]] && c_hex_12d_80l="0${c_hex_12d_80l}"
    [[ "${#c_hex_34d_80l}" -eq 1 ]] && c_hex_34d_80l="0${c_hex_34d_80l}"
    [[ "${#c_hex_56d_80l}" -eq 1 ]] && c_hex_56d_80l="0${c_hex_56d_80l}"
    c_hex_80l="#${c_hex_12d_80l}${c_hex_34d_80l}${c_hex_56d_80l}"
    c_rgb_80l="${c_rgb_12d_80l},${c_rgb_34d_80l},${c_rgb_56d_80l}"
    declare -g color"${i}"_lighter_80="$c_hex_80l"
    declare -g color"${i}"_lighter_80_rgb="$c_rgb_80l"

    c_rgb_12d_90l="$(( c_rgb_12d + "$((c_rgb_12d * 9 / 10))" ))"
    c_rgb_34d_90l="$(( c_rgb_34d + "$((c_rgb_34d * 9 / 10))" ))"
    c_rgb_56d_90l="$(( c_rgb_56d + "$((c_rgb_56d * 9 / 10))" ))"
    [[ "${c_rgb_12d_90l}" -ge 255 ]] && c_rgb_12d_90l=255
    [[ "${c_rgb_34d_90l}" -ge 255 ]] && c_rgb_34d_90l=255
    [[ "${c_rgb_56d_90l}" -ge 255 ]] && c_rgb_56d_90l=255
    printf -v c_hex_12d_90l '%x' "$c_rgb_12d_90l"
    printf -v c_hex_34d_90l '%x' "$c_rgb_34d_90l"
    printf -v c_hex_56d_90l '%x' "$c_rgb_56d_90l"
    [[ "${#c_hex_12d_90l}" -eq 1 ]] && c_hex_12d_90l="0${c_hex_12d_90l}"
    [[ "${#c_hex_34d_90l}" -eq 1 ]] && c_hex_34d_90l="0${c_hex_34d_90l}"
    [[ "${#c_hex_56d_90l}" -eq 1 ]] && c_hex_56d_90l="0${c_hex_56d_90l}"
    c_hex_90l="#${c_hex_12d_90l}${c_hex_34d_90l}${c_hex_56d_90l}"
    c_rgb_90l="${c_rgb_12d_90l},${c_rgb_34d_90l},${c_rgb_56d_90l}"
    declare -g color"${i}"_lighter_90="$c_hex_90l"
    declare -g color"${i}"_lighter_90_rgb="$c_rgb_90l"

    c_rgb_12d_10d="$(( c_rgb_12d - "$((c_rgb_12d / 10))" ))"
    c_rgb_34d_10d="$(( c_rgb_34d - "$((c_rgb_34d / 10))" ))"
    c_rgb_56d_10d="$(( c_rgb_56d - "$((c_rgb_56d / 10))" ))"
    printf -v c_hex_12d_10d '%x' "$c_rgb_12d_10d"
    printf -v c_hex_34d_10d '%x' "$c_rgb_34d_10d"
    printf -v c_hex_56d_10d '%x' "$c_rgb_56d_10d"
    [[ "${#c_hex_12d_10d}" -eq 1 ]] && c_hex_12d_10d="0${c_hex_12d_10d}"
    [[ "${#c_hex_34d_10d}" -eq 1 ]] && c_hex_34d_10d="0${c_hex_34d_10d}"
    [[ "${#c_hex_56d_10d}" -eq 1 ]] && c_hex_56d_10d="0${c_hex_56d_10d}"
    c_hex_10d="#${c_hex_12d_10d}${c_hex_34d_10d}${c_hex_56d_10d}"
    c_rgb_10d="${c_rgb_12d_10d},${c_rgb_34d_10d},${c_rgb_56d_10d}"
    declare -g color"${i}"_darker_10="$c_hex_10d"
    declare -g color"${i}"_darker_10_rgb="$c_rgb_10d"

    c_rgb_12d_20d="$(( c_rgb_12d - "$((c_rgb_12d / 5))" ))"
    c_rgb_34d_20d="$(( c_rgb_34d - "$((c_rgb_34d / 5))" ))"
    c_rgb_56d_20d="$(( c_rgb_56d - "$((c_rgb_56d / 5))" ))"
    printf -v c_hex_12d_20d '%x' "$c_rgb_12d_20d"
    printf -v c_hex_34d_20d '%x' "$c_rgb_34d_20d"
    printf -v c_hex_56d_20d '%x' "$c_rgb_56d_20d"
    [[ "${#c_hex_12d_20d}" -eq 1 ]] && c_hex_12d_20d="0${c_hex_12d_20d}"
    [[ "${#c_hex_34d_20d}" -eq 1 ]] && c_hex_34d_20d="0${c_hex_34d_20d}"
    [[ "${#c_hex_56d_20d}" -eq 1 ]] && c_hex_56d_20d="0${c_hex_56d_20d}"
    c_hex_20d="#${c_hex_12d_20d}${c_hex_34d_20d}${c_hex_56d_20d}"
    c_rgb_20d="${c_rgb_12d_20d},${c_rgb_34d_20d},${c_rgb_56d_20d}"
    declare -g color"${i}"_darker_20="$c_hex_20d"
    declare -g color"${i}"_darker_20_rgb="$c_rgb_20d"

    c_rgb_12d_30d="$(( c_rgb_12d - "$((c_rgb_12d * 3 / 10))" ))"
    c_rgb_34d_30d="$(( c_rgb_34d - "$((c_rgb_34d * 3 / 10))" ))"
    c_rgb_56d_30d="$(( c_rgb_56d - "$((c_rgb_56d * 3 / 10))" ))"
    printf -v c_hex_12d_30d '%x' "$c_rgb_12d_30d"
    printf -v c_hex_34d_30d '%x' "$c_rgb_34d_30d"
    printf -v c_hex_56d_30d '%x' "$c_rgb_56d_30d"
    [[ "${#c_hex_12d_30d}" -eq 1 ]] && c_hex_12d_30d="0${c_hex_12d_30d}"
    [[ "${#c_hex_34d_30d}" -eq 1 ]] && c_hex_34d_30d="0${c_hex_34d_30d}"
    [[ "${#c_hex_56d_30d}" -eq 1 ]] && c_hex_56d_30d="0${c_hex_56d_30d}"
    c_hex_30d="#${c_hex_12d_30d}${c_hex_34d_30d}${c_hex_56d_30d}"
    c_rgb_30d="${c_rgb_12d_30d},${c_rgb_34d_30d},${c_rgb_56d_30d}"
    declare -g color"${i}"_darker_30="$c_hex_30d"
    declare -g color"${i}"_darker_30_rgb="$c_rgb_30d"

    c_rgb_12d_40d="$(( c_rgb_12d - "$((c_rgb_12d * 2 / 5))" ))"
    c_rgb_34d_40d="$(( c_rgb_34d - "$((c_rgb_34d * 2 / 5))" ))"
    c_rgb_56d_40d="$(( c_rgb_56d - "$((c_rgb_56d * 2 / 5))" ))"
    printf -v c_hex_12d_40d '%x' "$c_rgb_12d_40d"
    printf -v c_hex_34d_40d '%x' "$c_rgb_34d_40d"
    printf -v c_hex_56d_40d '%x' "$c_rgb_56d_40d"
    [[ "${#c_hex_12d_40d}" -eq 1 ]] && c_hex_12d_40d="0${c_hex_12d_40d}"
    [[ "${#c_hex_34d_40d}" -eq 1 ]] && c_hex_34d_40d="0${c_hex_34d_40d}"
    [[ "${#c_hex_56d_40d}" -eq 1 ]] && c_hex_56d_40d="0${c_hex_56d_40d}"
    c_hex_40d="#${c_hex_12d_40d}${c_hex_34d_40d}${c_hex_56d_40d}"
    c_rgb_40d="${c_rgb_12d_40d},${c_rgb_34d_40d},${c_rgb_56d_40d}"
    declare -g color"${i}"_darker_40="$c_hex_40d"
    declare -g color"${i}"_darker_40_rgb="$c_rgb_40d"

    c_rgb_12d_50d="$(( c_rgb_12d - "$((c_rgb_12d / 2))" ))"
    c_rgb_34d_50d="$(( c_rgb_34d - "$((c_rgb_34d / 2))" ))"
    c_rgb_56d_50d="$(( c_rgb_56d - "$((c_rgb_56d / 2))" ))"
    printf -v c_hex_12d_50d '%x' "$c_rgb_12d_50d"
    printf -v c_hex_34d_50d '%x' "$c_rgb_34d_50d"
    printf -v c_hex_56d_50d '%x' "$c_rgb_56d_50d"
    [[ "${#c_hex_12d_50d}" -eq 1 ]] && c_hex_12d_50d="0${c_hex_12d_50d}"
    [[ "${#c_hex_34d_50d}" -eq 1 ]] && c_hex_34d_50d="0${c_hex_34d_50d}"
    [[ "${#c_hex_56d_50d}" -eq 1 ]] && c_hex_56d_50d="0${c_hex_56d_50d}"
    c_hex_50d="#${c_hex_12d_50d}${c_hex_34d_50d}${c_hex_56d_50d}"
    c_rgb_50d="${c_rgb_12d_50d},${c_rgb_34d_50d},${c_rgb_56d_50d}"
    declare -g color"${i}"_darker_50="$c_hex_50d"
    declare -g color"${i}"_darker_50_rgb="$c_rgb_50d"

    c_rgb_12d_60d="$(( c_rgb_12d - "$((c_rgb_12d * 3 / 5))" ))"
    c_rgb_34d_60d="$(( c_rgb_34d - "$((c_rgb_34d * 3 / 5))" ))"
    c_rgb_56d_60d="$(( c_rgb_56d - "$((c_rgb_56d * 3 / 5))" ))"
    printf -v c_hex_12d_60d '%x' "$c_rgb_12d_60d"
    printf -v c_hex_34d_60d '%x' "$c_rgb_34d_60d"
    printf -v c_hex_56d_60d '%x' "$c_rgb_56d_60d"
    [[ "${#c_hex_12d_60d}" -eq 1 ]] && c_hex_12d_60d="0${c_hex_12d_60d}"
    [[ "${#c_hex_34d_60d}" -eq 1 ]] && c_hex_34d_60d="0${c_hex_34d_60d}"
    [[ "${#c_hex_56d_60d}" -eq 1 ]] && c_hex_56d_60d="0${c_hex_56d_60d}"
    c_hex_60d="#${c_hex_12d_60d}${c_hex_34d_60d}${c_hex_56d_60d}"
    c_rgb_60d="${c_rgb_12d_60d},${c_rgb_34d_60d},${c_rgb_56d_60d}"
    declare -g color"${i}"_darker_60="$c_hex_60d"
    declare -g color"${i}"_darker_60_rgb="$c_rgb_60d"

    c_rgb_12d_70d="$(( c_rgb_12d - "$((c_rgb_12d * 7 / 10))" ))"
    c_rgb_34d_70d="$(( c_rgb_34d - "$((c_rgb_34d * 7 / 10))" ))"
    c_rgb_56d_70d="$(( c_rgb_56d - "$((c_rgb_56d * 7 / 10))" ))"
    printf -v c_hex_12d_70d '%x' "$c_rgb_12d_70d"
    printf -v c_hex_34d_70d '%x' "$c_rgb_34d_70d"
    printf -v c_hex_56d_70d '%x' "$c_rgb_56d_70d"
    [[ "${#c_hex_12d_70d}" -eq 1 ]] && c_hex_12d_70d="0${c_hex_12d_70d}"
    [[ "${#c_hex_34d_70d}" -eq 1 ]] && c_hex_34d_70d="0${c_hex_34d_70d}"
    [[ "${#c_hex_56d_70d}" -eq 1 ]] && c_hex_56d_70d="0${c_hex_56d_70d}"
    c_hex_70d="#${c_hex_12d_70d}${c_hex_34d_70d}${c_hex_56d_70d}"
    c_rgb_70d="${c_rgb_12d_70d},${c_rgb_34d_70d},${c_rgb_56d_70d}"
    declare -g color"${i}"_darker_70="$c_hex_70d"
    declare -g color"${i}"_darker_70_rgb="$c_rgb_70d"

    c_rgb_12d_80d="$(( c_rgb_12d - "$((c_rgb_12d * 4 / 5))" ))"
    c_rgb_34d_80d="$(( c_rgb_34d - "$((c_rgb_34d * 4 / 5))" ))"
    c_rgb_56d_80d="$(( c_rgb_56d - "$((c_rgb_56d * 4 / 5))" ))"
    printf -v c_hex_12d_80d '%x' "$c_rgb_12d_80d"
    printf -v c_hex_34d_80d '%x' "$c_rgb_34d_80d"
    printf -v c_hex_56d_80d '%x' "$c_rgb_56d_80d"
    [[ "${#c_hex_12d_80d}" -eq 1 ]] && c_hex_12d_80d="0${c_hex_12d_80d}"
    [[ "${#c_hex_34d_80d}" -eq 1 ]] && c_hex_34d_80d="0${c_hex_34d_80d}"
    [[ "${#c_hex_56d_80d}" -eq 1 ]] && c_hex_56d_80d="0${c_hex_56d_80d}"
    c_hex_80d="#${c_hex_12d_80d}${c_hex_34d_80d}${c_hex_56d_80d}"
    c_rgb_80d="${c_rgb_12d_80d},${c_rgb_34d_80d},${c_rgb_56d_80d}"
    declare -g color"${i}"_darker_80="$c_hex_80d"
    declare -g color"${i}"_darker_80_rgb="$c_rgb_80d"

    c_rgb_12d_90d="$(( c_rgb_12d - "$((c_rgb_12d * 9 / 10))" ))"
    c_rgb_34d_90d="$(( c_rgb_34d - "$((c_rgb_34d * 9 / 10))" ))"
    c_rgb_56d_90d="$(( c_rgb_56d - "$((c_rgb_56d * 9 / 10))" ))"
    printf -v c_hex_12d_90d '%x' "$c_rgb_12d_90d"
    printf -v c_hex_34d_90d '%x' "$c_rgb_34d_90d"
    printf -v c_hex_56d_90d '%x' "$c_rgb_56d_90d"
    [[ "${#c_hex_12d_90d}" -eq 1 ]] && c_hex_12d_90d="0${c_hex_12d_90d}"
    [[ "${#c_hex_34d_90d}" -eq 1 ]] && c_hex_34d_90d="0${c_hex_34d_90d}"
    [[ "${#c_hex_56d_90d}" -eq 1 ]] && c_hex_56d_90d="0${c_hex_56d_90d}"
    c_hex_90d="#${c_hex_12d_90d}${c_hex_34d_90d}${c_hex_56d_90d}"
    c_rgb_90d="${c_rgb_12d_90d},${c_rgb_34d_90d},${c_rgb_56d_90d}"
    declare -g color"${i}"_darker_90="$c_hex_90d"
    declare -g color"${i}"_darker_90_rgb="$c_rgb_90d"
  done
}

