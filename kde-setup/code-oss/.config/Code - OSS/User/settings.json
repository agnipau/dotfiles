{
    "editor.smoothScrolling": true,
    "editor.cursorBlinking": "solid",
    "editor.fontLigatures": true,
    "workbench.statusBar.feedback.visible": false,
    "window.menuBarVisibility": "toggle",
    "search.smartCase": true,
    "vim.neovimPath": "/usr/bin/nvim",
    "vim.leader": "<space>",
    "vim.hlsearch": true,
    "vim.enableNeovim": true,
    "vim.useSystemClipboard": true,
    "gitProjectManager.baseProjectsFolders": [
        "/home/matte/progetti"
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<leader>", "<leader>"],
            "after": ["/"]
        },
        {
            "before": ["<leader>", "m", "o"],
            "commands": [":nohlsearch"]
        },
        {
            "before": ["<c-d>"],
            "after": ["}"]
        },
        {
            "before": ["<c-u>"],
            "after": ["{"]
        },
        {
            "before": ["Z", "Z"],
            "commands": [":w"]
        },
        {
            "before": ["Z", "Q"],
            "commands": [":q!"]
        },
        {
            "before": ["J"],
            "after": ["j", "j", "j"]
        },
        {
            "before": ["K"],
            "after": ["k", "k", "k"]
        },
        {
            "before": ["H"],
            "after": ["0"]
        },
        {
            "before": ["L"],
            "after": ["$"]
        },
        {
            "before": [","],
            "after": [":"]
        },
        {
            "before": ["è"],
            "after": ["."]
        },
        {
            "before": ["!"],
            "after": [":", "!"]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": ["<leader>", "<leader>"],
            "after": ["/"]
        },
        {
            "before": ["<leader>", "m", "o"],
            "commands": [":nohlsearch"]
        },
        {
            "before": ["<c-d>"],
            "after": ["}"]
        },
        {
            "before": ["<c-u>"],
            "after": ["{"]
        },
        {
            "before": ["Z", "Z"],
            "commands": [":w"]
        },
        {
            "before": ["Z", "Q"],
            "commands": [":q!"]
        },
        {
            "before": ["J"],
            "after": ["j", "j", "j"]
        },
        {
            "before": ["K"],
            "after": ["k", "k", "k"]
        },
        {
            "before": ["H"],
            "after": ["0"]
        },
        {
            "before": ["L"],
            "after": ["$"]
        },
        {
            "before": [","],
            "after": [":"]
        },
        {
            "before": ["è"],
            "after": ["."]
        },
        {
            "before": ["!"],
            "after": [":", "!"]
        }
    ],
    "git.confirmSync": false,
    "git.autofetch": true,
    "explorer.confirmDelete": false,
    "window.zoomLevel": 0,
    "workbench.iconTheme": "material-icon-theme",
    "csharp.format.enable": false,
    "extensions.ignoreRecommendations": false,
    "terminal.integrated.fontFamily": "'Iosevka Term SS07'",
    "workbench.sideBar.location": "left",
    "workbench.colorTheme": "Gruvbox Minor Dark Medium",
    "todo-tree.defaultHighlight": {},
    "window.titleBarStyle": "native",
    "editor.fontFamily": "'Iosevka SS07', 'Roboto Mono', 'IBM Plex Mono', 'SF Mono', 'Fira Code', 'Operator Mono Lig Book', Menlo, Consolas, DejaVu Sans Mono, monospace",
    "vim.easymotionMarkerFontFamily": "'Iosevka SS07', 'Roboto Mono', 'IBM Plex Mono', 'SF Mono', 'Fira Code', 'Operator Mono Lig Book', Menlo, Consolas, DejaVu Sans Mono, monospace",
    "editor.lineNumbers": "relative",
    "editor.fontSize": 16,
    "terminal.integrated.fontSize": 16,
    "vim.easymotionMarkerFontSize": "16"
}